    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "homepage": "",
  "engines": {
    "node": "4.5.0"
  },
  "babelrc": true,
  "keywords": [
    "nodejs",
    "reactjs",
    "mobx",
    "api",
    "boilerplate",
    "express",
    "restful",
    "api",
    "es6"
  ]
